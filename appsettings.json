{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning",
      "SpanTrace": "Information"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File",
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [http-nio-18080-exec-10] {Level:u4} {SourceContext} - msg {Message:lj} spanContext [traceId={TraceId}, spanId={SpanId}]{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/frm.log",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [http-nio-18080-exec-10] {Level:u4} {SourceContext} - msg {Message:lj} spanContext [traceId={TraceId}, spanId={SpanId}]{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithThreadId",
      "WithMachineName",
      "WithSpan"
    ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=mssql;Database=Vunet;User=sa;Password=Vunetise#a78142025;Encrypt=True;TrustServerCertificate=True"
  }
}